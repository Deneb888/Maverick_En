<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <runtime>
    <generatePublisherEvidence enabled="false"/>
  </runtime>
  <appSettings>
    <add key="xdvalue" value="200"/>
    <add key="currks" value="4"/>
